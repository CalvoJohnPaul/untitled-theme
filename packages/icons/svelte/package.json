{
	"name": "@untitled-theme/icons-svelte",
	"type": "module",
	"version": "0.14.2",
	"license": "MIT",
	"description": "Untitled UI icons for Svelte",
	"keywords": [
		"svg",
		"icon",
		"icons",
		"Svelte",
		"SvelteKit",
		"Untitled UI"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JohnPaulCalvo/untitled-theme.git",
		"directory": "packages/icons/svelte"
	},
	"bugs": {
		"url": "https://github.com/JohnPaulCalvo/untitled-theme/issues"
	},
	"author": {
		"name": "John Paul Calvo",
		"email": "calvojp92@gmail.com"
	},
	"main": "dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"svelte": "./dist/*"
		},
		"./package.json": {
			"default": "./package.json"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prebuild": "pnpm --filter @untitled-theme/scripts run generate:icons -f svelte",
		"build": "svelte-package --input ./src",
		"release": "release-it"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.5.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tsconfig/svelte": "^5.0.5",
		"release-it": "^19.0.4",
		"svelte": "^5.39.2",
		"svelte-preprocess": "^6.0.3",
		"typescript": "^5.9.2",
		"vite": "^7.1.6"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"release-it": {
		"hooks": {
			"after:bump": [
				"pnpm build"
			]
		},
		"git": {
			"commitMessage": "chore: release ${npm.name} v${version}",
			"tagName": "${npm.name}@${version}"
		},
		"github": {
			"release": false
		}
	}
}
